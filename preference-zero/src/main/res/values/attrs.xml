<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="fontPath" format="string"/>

    <declare-styleable name="CustomTabLayout">
        <attr name="fontPath"/>
    </declare-styleable>

    <declare-styleable name="PreferenceZero">
        <attr name="ep_key" format="string"/>
        <attr name="ep_title" format="string"/>
        <attr name="ep_summary" format="string"/>
        <attr name="ep_default" format="string"/>
        <attr name="ep_style" format="reference"/>
        <attr name="ep_dependency" format="string"/>
        <attr name="ep_icon" format="reference"/>
        <attr name="ep_detail_on" format="string"/>
        <attr name="ep_detail_off" format="string"/>
        <attr name="ep_min_value" format="integer"/>
        <attr name="ep_max_value" format="integer"/>
        <attr name="ep_show_value" format="boolean"/>
        <attr name="android:entries"/>
        <attr name="android:entryValues"/>
    </declare-styleable>

</resources>